<Window x:Class="WpfApp1.DialogWindows.AddMatirial"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WpfApp1.DialogWindows"
        mc:Ignorable="d"
        Title="AddMatirial" Height="450" Width="800">
    <Grid>       
        <Grid.ColumnDefinitions>
            <ColumnDefinition/>
            <ColumnDefinition/>
        </Grid.ColumnDefinitions>

        <Grid.RowDefinitions>
            <RowDefinition/>
            <RowDefinition/>
            <RowDefinition/>
            <RowDefinition/>
            <RowDefinition/>
            <RowDefinition/>
            <RowDefinition/>
            <RowDefinition/>
        </Grid.RowDefinitions>       

        <TextBlock Grid.Column="0" Grid.Row="0">Имя</TextBlock>
        <TextBlock Grid.Column="0" Grid.Row="1">Сv</TextBlock>
        <TextBlock Grid.Column="0" Grid.Row="2">Xv</TextBlock>
        <TextBlock Grid.Column="0" Grid.Row="3">Yv</TextBlock>
        <TextBlock Grid.Column="0" Grid.Row="4">Mv</TextBlock>
        <TextBlock Grid.Column="0" Grid.Row="5">Kmv</TextBlock>
        <TextBlock Grid.Column="0" Grid.Row="6">Kpv</TextBlock>

        <TextBox Grid.Column="1" Grid.Row="0"></TextBox>
        <TextBox Grid.Column="1" Grid.Row="1"></TextBox>
        <TextBox Grid.Column="1" Grid.Row="2"></TextBox>
        <TextBox Grid.Column="1" Grid.Row="3"></TextBox>
        <TextBox Grid.Column="1" Grid.Row="4"></TextBox>
        <TextBox Grid.Column="1" Grid.Row="5"></TextBox>
        <TextBox Grid.Column="1" Grid.Row="6"></TextBox>
                                 
        <Button Grid.Column="1" Grid.Row="7" Content="Add"/>

    </Grid>
</Window>
